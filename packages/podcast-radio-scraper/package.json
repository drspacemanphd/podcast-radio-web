  "description": "A Generic Scraping Component That Kicks Off Jobs Associated with Changes in Podcast Feeds",
  "keywords": [
    "Podcast",
    "Radio",
    "Scrape"
  ],
  "author": "drspacemanphd",
  "homepage": "https://github.com/drspacemanphd/podcast-radio-web#readme",
  "license": "ISC",
  "main": "./dist/index",
  "types": "./dist/index",
  "repository": "ssh://git@github.com/drspacemanphd/podcast-radio-web.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "clean": "rm -rf ./dist/",
