    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@drspacemanphd/podcast-radio-interfaces": "^0.0.8",
    "@drspacemanphd/podcast-radio-model": "^0.0.8",
    "got": "^11.8.1",
